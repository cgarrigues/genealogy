objectclass ( 1.3.6.1.4.1.19106.1.2.1
	NAME 'gedcomSour'
	DESC 'a gedcom source file'
	STRUCTURAL
	MUST (objectClass $ uniqueIdentifier )
	MAY ( rawData $ title $ version $ corp $ note $ publication $ author )
	)

objectclass ( 1.3.6.1.4.1.19106.1.2.2
	NAME 'gedcomIndi'
	DESC 'a gedcom individual'
	STRUCTURAL
	MUST (objectClass $ uniqueIdentifier )
	MAY ( cn $ givenName $ sn $ initials $ nameDNs $ gender $ birthDN $ baptismDN $ deathDN $ motherDN $ fatherDN $ eventDNs $ sourceDNs)
	)

