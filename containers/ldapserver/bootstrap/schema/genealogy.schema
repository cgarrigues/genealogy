objectclass ( 1.3.6.1.4.1.19106.1.2.1
	NAME 'gedcomSource'
	DESC 'a gedcom source file'
	STRUCTURAL
	MUST (objectClass )
	MAY ( rawData $ title $ version $ corp $ note $ publication $ author $ uniqueIdentifier)
	)

objectclass ( 1.3.6.1.4.1.19106.1.2.2
	NAME 'gedcomIndividual'
	DESC 'a gedcom individual'
	STRUCTURAL
	MUST (objectClass $ uniqueIdentifier )
	MAY ( cn $ givenName $ sn $ initials $ nameDNs $ gender $ birthDN $ baptismDN $ deathDN $ motherDN $ fatherDN $ sourceDNs)
	)

objectclass ( 1.3.6.1.4.1.19106.1.2.3
	NAME 'gedcomName'
	DESC 'a gedcom name'
	STRUCTURAL
	MUST (objectClass $ sn )
	MAY ( givenName $ sn $ initials )
	)

objectclass ( 1.3.6.1.4.1.19106.1.2.4
	NAME 'gedcomEvent'
	DESC 'a gedcom event'
	STRUCTURAL
	MUST (objectClass $ description)
	MAY ( gedcomDate $ year $ month $ day $ relativeToDate $ gedcomPlace $ sourceDNs $ baddata)
	)

objectclass ( 1.3.6.1.4.1.19106.1.2.5
	NAME 'gedcomBirth'
	DESC 'a gedcom birth'
	AUXILIARY
	MUST (individualDN)
	)

